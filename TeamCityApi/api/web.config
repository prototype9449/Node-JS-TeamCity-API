<configuration>
     <system.webServer>  

     
	 <handlers>
         <add name="iisnode" path="application.js" verb="*" modules="iisnode" />
       </handlers>
      
        <rewrite>  
        <rules>  
         <rule name="DynamicContent">  
            <action type="Rewrite" url="application.js" />   
         </rule>  
       </rules>  
     </rewrite>  
    </system.webServer>
  </configuration>